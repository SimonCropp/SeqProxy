<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageProjectUrl>https://github.com/SimonCropp/SeqProxy</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="MockPoster.cs" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="7.0.0" />
    <PackageReference Include="ProjectDefaults" Version="1.0.84" PrivateAssets="all" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" Condition="$(Configuration) == 'Release'" />


    <Using Include="Microsoft.AspNetCore.Http" />
    <Using Include="System.Security.Claims" />
    <Using Include="System.Web" />
    <Using Include="Microsoft.Net.Http.Headers" />
    <Using Include="SeqProxy" />
    <Using Include="Microsoft.AspNetCore.Authentication" />
    <Using Include="Microsoft.AspNetCore.Authorization" />
    <Using Include="Microsoft.AspNetCore.Builder" />
        
  </ItemGroup>
</Project>